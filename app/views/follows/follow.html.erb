<% @page_title = "フォロー一覧" %>
<div class="follows-wrapper">
  <div class="container">
    <h3>フォロー中</h3>
    <% if @followings.present? %>
      <%= render "share/follow_list", users: @followings %>
    <% else %>
      <div class="notfind">
        <p>フォロー中のユーザーがいません。</p>
      </div>
    <% end %>
  </div>
</div>
